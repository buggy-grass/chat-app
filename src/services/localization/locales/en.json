              },
              "meshSize": {
                "header": "Mesh Size",
                "description": "Specify the maximum number of vertices for the mesh output."
              },
              "texturingKeepUnseenFaces": {
                "header": "Texture Protection",
                "description": "Enables preservation of mesh textures that are not seen by any camera."
              },
              "useThreeDMesh": {
                "header": "Use 3D Mesh",
                "description": "Use a full 3D mesh instead of a 2.5D mesh to compute the orthophoto. This option is slightly faster and yields similar results on flat areas."
