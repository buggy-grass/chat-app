        "process": {
          "component": {
            "pointCloud": {
              "pointCloudQuality": {
                "header": "Point Cloud Quality",
                "description": "Adjust the point cloud quality for better photogrammetric analysis. Higher quality results in denser and more accurate point clouds, which improves the reconstruction but requires more memory and processing time. Each increase in quality approximately quadruples the processing time."
              },
              "pointCloudFilter": {
                "header": "Point Cloud Filtering",
                "description": "Filters the point cloud by removing outliers that deviate by more than N standard deviations from the local mean, which can improve the precision of photogrammetric results. Set to 0 to disable filtering."
              },
              "pointCloudRectify": {
                "header": "Point Cloud Rectification",
                "description": "Rectify the ground in the point cloud for better photogrammetric surface modeling. Misclassified ground points will be reclassified, and gaps will be filled. Essential for creating accurate Digital Terrain Models (DTMs)."
              },
              "pointCloudClassify": {
                "header": "Point Cloud Classification",
                "description": "Classify point cloud outputs based on photogrammetric categories such as vegetation, buildings, and ground. The behavior can be controlled by adjusting the DTM and DSM parameters."
              },
              "pointCloudSample": {
                "header": "Point Cloud Sampling",
                "description": "Resample the point cloud by retaining only a single point within an N-meter radius. Useful for reducing redundancy in dense point clouds for faster processing in photogrammetric workflows. Set to 0 to disable sampling."
              },
              "pointCloudQualities": {
                "ultra": "Ultra High",
                "high": "High",
                "medium": "Medium",
                "low": "Low",
                "veryLow": "Very Low"
              },
